{
    "files.exclude": {
        "*.js": {
            "when": "$(basename).ts"
        },
        "*.js.map": {
            "when": "$(basename)"
        },
        "*.mjs": {
            "when": "$(basename).mts"
        },
        "*.mjs.map": {
            "when": "$(basename)"
        }
    }
}